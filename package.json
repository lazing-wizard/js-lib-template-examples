{
  "name": "js-lib-template-examples",
  "version": "1.0.0",
  "description": "JavaScript library template with example viewer",
  "keywords": [],
  "author": "Lazing Wizard",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lazing-wizard/js-lib-template-examples"
  },
  "type": "module",
  "module": "dist/example_lib.js",
  "main": "dist/example_lib.cjs",
  "unpkg": "dist/example_lib.umd.js",
  "jsdelivr": "dist/example_lib.umd.js",
  "exports": {
    ".": {
      "import": "./dist/example_lib.js",
      "require": "./dist/example_lib.cjs",
      "default": "./dist/example_lib.js"
    }
  },
  "files": ["dist/example_lib.js", "dist/example_lib.cjs", "dist/example_lib.umd.js"],
  "sideEffects": false,
  "prepublishOnly": "npm run build",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "start": "vite",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist package-lock.json node_modules",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "preview": "vite build && vite preview",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "vite": "^7.1.3",
    "globals": "^16.3.0",
    "eslint": "^9.34.0",
    "@eslint/js": "^9.34.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.1.1",
    "@types/jest": "30.0.0",
    "eslint-plugin-jest": "^29.0.1",
    "prettier": "^3.6.2",
    "eslint-config-prettier": "^10.1.8",
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "babel-jest": "^30.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "chokidar": "^4.0.3",
    "rimraf": "^6.0.1"
  }
}
